view {
    box-sizing: border-box;
}
.banner_wrapper{
    width: 100%;
    height: 300rpx;
}
.banner_wrapper>image{
    width: 100%;
    height: 100%;
}
.title_wrapper{
    width: 100%;
    height:92rpx;
    background:#fa7d04;
    line-height: 92rpx;
}
.title_wrapper>text:nth-child(1){
    padding-left: 38rpx;
    font-family:PingFangSC-Medium;
    font-size:32rpx;
    color:#ffffff;
    letter-spacing:0;
    text-align:left;
}
.title_wrapper>view{
    font-family:PingFangSC-Semibold;
    font-size:24rpx;
    color:#ffffff;
    letter-spacing:0;
    text-align:left;
    border:1px solid #ffffff;
    border-radius:200rpx;
    width:156rpx;
    height:44rpx;
    line-height: 44rpx;
    margin-left: 20rpx;
    display: inline-block;
    text-align: center;
}
.title_wrapper image{
    width:8rpx;
    height: 10rpx; 
}
.city_wrapper{
    width: 100%;
    display: flex;
    justify-content:space-between;
    padding-left: 24rpx;
    padding-right: 24rpx;
}
.city_wrapper image{
    width: 20rpx;
    height: 12rpx;
    top:35rpx;
    position: relative;
}
.chose-result {
    background:#f6f8fb;
    border:1px solid #ececec;
    border-radius:3px;
    width:322rpx;
    height:80rpx;
    display: inline-block;
    font-family:PingFangSC-Regular;
    font-size:28rpx;
    color:#333333;
    letter-spacing:0;
    text-align:left;
    line-height: 80rpx;
    padding-left: 24rpx;
     padding-right: 24rpx;
     display: flex;
     justify-content: space-between
}
.sale_city{
    width: 322rpx;

}
.reg_city{
    width: 322rpx;
}
.city-title{
   font-family:PingFangSC-Regular;
   font-size:24rpx;
   text-align: left;
   color:#24253d;
   margin-top: 20rpx;
   margin-bottom: 10rpx; 
}

.select-wrapper{
    width: 100%;
    padding-left: 24rpx;
    padding-right: 24rpx;
    margin-top: 20rpx;
}
.text{
    font-family:PingFangSC-Regular;
    font-size:24rpx;
    color:#24253d;
    text-align:left;

}
.select{
   background:#f6f8fb;
   border:1px solid #ececec;
   border-radius:3px;
   width:100%;
   height:80rpx;
   display: inline-block;
   font-family:PingFangSC-Regular;
   font-size:28rpx;
   color:#c2ccdb;
   letter-spacing:0;
   text-align:left;
   line-height: 80rpx;
   padding-left: 24rpx;
   padding-right: 24rpx;
   margin-top: 10rpx;
}
.mile{
    display: flex;
    justify-content: space-between
}
.select>input{
    height: 80rpx;
    line-height: 80rpx;
    width: 322rpx;
    color:#c2ccdb;
}
::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #c2ccdb;
}
.select>text {
    display: inline-block;
}
.button_wrapper{
    padding-left: 24rpx;
    padding-right: 24rpx;
    margin-top: 20rpx;
}
.button{
    background:#fb7f02;
    border-radius:3px;
    width:100%;
    height:100rpx;
    font-family:PingFangSC-Medium;
    font-size:30rpx;
    color:#ffffff;
    letter-spacing:4rpx;
    text-align:center;
    line-height: 100rpx;
}
