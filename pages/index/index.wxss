view{
    box-sizing: border-box;
}
.swiper-image swiper {
    width: 100%;
    height: 346rpx;
}
.swiper-image swiper-item {
    width: 100%;
    height: 346rpx;
}
.swiper-image .slide-image {
 width: 100%;
 height: 346rpx;
}
.search-container{
    width: 100%;
    height: 80rpx;
    position: absolute;
    top:20rpx;
    z-index: 100;
}
.search-wrapper{
    background:#ffffff;
    box-shadow:0 2px 4px 0 rgba(102, 102, 102, 0.50);
    width:674rpx;
    height:80rpx;
    display: flex;
    margin-left: 38rpx;
}
.address-wrapper{
   text-align: center;
   line-height: 80rpx;
    height:80rpx;
    width:160rpx;
}
.address-wrapper>text{
    font-family:PingFangSC-Regular;
    font-size:28rpx;
    color:#333333;
    letter-spacing:0;
    text-align:center;
    width: 56rpx;
    height: 40rpx;
    line-height: 40rpx;
}
.drop-down{
   width:14rpx;
   height:8rpx;
   position: relative;
   top:-3rpx;
   left:10rpx;
}
.line-small{
    width: 1px ;
    height: 26rpx;
    border-left:1px solid #dedede;
    position: relative;
    top:26rpx;
}

.input-wrapper{
    width: 530rpx;
    font-family:PingFangSC-Regular;
    font-size:24rpx;
    color:#a6b1c2;
    letter-spacing:0;
    height: 100%;
    padding-left: 30rpx;
    display: inline-block;
}
.input-wrapper input {
    font-family: PingFangSC-Regular;
    font-size: 24rpx;
    color: #a6b1c2;
    letter-spacing: 0;
    height: 80rpx;
    line-height: 80rpx;
    width: 442rpx;
    float: right;
}
.search-icon {
    width:24rpx;
    height:24rpx;
    position: relative;
    top:26rpx;
}

.icon_list{
    width: 100%;
    height:188rpx;
    display: flex;
    justify-content: space-around;
    padding-top: 25rpx;

}
.icon_list>view{
    width: 96rpx;
    height: 100%;
}
.icon_list>view>image{
    width:96rpx;
    height: 96rpx; 
}
.icon_list>view>text{
    font-family:PingFangSC-Regular;
    font-size:24rpx;
    height: 34rpx;
    color:#333333;
    letter-spacing:0;
    text-align:center;
    position: relative;
    top:-10rpx;
}
.line{
    width: 100%;
    height: 15rpx;
    background: #F9F9F9;
}
.select-wrapper{
    width: 100%;
    height: 80rpx;
    display: flex;
    line-height: 80rpx;
    justify-content: space-around;
}
.item_wrapper{
   text-align: center;
   width: 188rpx;
}
.item_wrapper>text{
    font-family:PingFangSC-Regular;
    font-size:24rpx;
    color:#333333;
    letter-spacing:0;
    text-align:left;
}

.good_wrapper{
    width: 100%;
    height: 218rpx;
    border-top: 1px solid #F9F9F9;
    padding-left: 24rpx;
    padding-right: 24rpx;
    display: flex;
    padding-top: 14rpx;
}
.img_wrapper{
   width:262rpx;
   height:160rpx;
   background:#ddd;
   text-align:center;

}
.img_wrapper>image{
    width: 72%;
    height: 100%;
}
.info_wrapper{
    width: 400rpx;
    padding-left: 24rpx;
    height: 160rpx;
}
.good_name{
    width: 100%;
    line-height:40rpx;
}
.good_name>view{
    font-family:PingFangSC-Semibold;
    font-size:30rpx;
    color:#333333;
    letter-spacing:0;
    line-height:30rpx;
    text-align:left;
    display: inline-block;
    font-weight: bold;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width: 290rpx;
}
.good_name>text{
    border:1rpx solid #f35a24;
    border-radius:4rpx;
    width:82rpx;
    height:28rpx;
    font-family:PingFangSC-Semibold;
    font-size:20rpx;
    color:#ff703e;
    letter-spacing:0;
    text-align:left;
    position: relative;
    top:-5rpx;
    padding-left: 8rpx;
    padding-right:8rpx;
}
.old{
    font-family:PingFangSC-Semibold;
    font-size:30rpx;
    color:#333333;
    letter-spacing:0;
    line-height:30rpx;
    text-align:left;
    display: inline-block;
    font-weight: bold;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    width: 364rpx;
    line-height: 1;
}
.address-good{
    line-height: 30rpx;
}
.address-good>text{
    font-family:PingFangSC-Regular;
    font-size:24rpx;
    color:#a6b1c2;
    letter-spacing:0;
    text-align:left;
    margin-left: 10rpx;
}
.address-good>text:nth-child(1){
    margin-left: 0;
}

.price_info{
    font-family:PingFangSC-Semibold;
    font-size:24rpx;
    color:#ff8800;
    letter-spacing:0;
    text-align:left;
    line-height: 44rpx;
}
.price_info>text{
    margin-left: 10rpx;
}
.price_info>text:nth-child(1){
    margin-left: 0;
}
.price_info>text:nth-child(2){
    font-size: 30rpx;
}
.loading{
    width: 100%;
    height: 30rpx;
    line-height: 30rpx;
    text-align: center;
    font-size: 30rpx;
    padding-bottom: 10rpx;
}
.operate_wrapper{
    width:80rpx;
    position:fixed;
    right:20rpx;
}
.operate_wrapper>view{
    width:80rpx;
    height:80rpx;
    margin-top:20rpx;
    text-align:center;
}
.operate_wrapper>view:nth-child(1){
    width:80rpx;
    height:80rpx;
    margin-top:0;
}
.operate_wrapper>view:nth-child(3) {
    position: relative
}
.operate_wrapper>view:nth-child(1)>image{
    width:100rpx;
    height:100rpx;
    margin-top:15rpx;
}
.operate_wrapper>view:nth-child(2)>image{
    width:100rpx;
    height:100rpx;
    margin-top:15rpx;
}
.operate_wrapper>view:nth-child(3)>image{
    width:100rpx;
    height:100rpx;
    margin-top:15rpx;
}
.numCollection{
   width:40rpx;
   height:40rpx;
   line-height:40rpx;
   color:#fff;
   background:red;
   text-align:center;
   border-radius:50%;
   position:absolute;
   display:inline-block;
   font-size:24rpx;
   top:19rpx;
   right:-10rpx
}